    unsigned OpNum,
    const MCSubtargetInfo &STI) const {
  switch (MI.getOpcode()) {
    case Rymo::ADDI:
    case Rymo::FMUL:
    case Rymo::JALR: {
      switch (OpNum) {
      case 0:
        // op: rd
